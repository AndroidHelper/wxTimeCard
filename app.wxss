/**app.wxss**/
page{height: 100%;background:#FFFFFF;font-family: PingFangSC-Regular;}
/* 上下布局 */
.content_wrap{min-height: 100%;display: flex;flex-direction: column;}
.content_wrap .content{flex: 1;background:#fff;}
.content_wrap .footer{background: #fff;height: 64rpx; line-height: 40rpx; text-align: center;}
/* 面板描述 */
.item_desc{display: flex; padding: 20rpx 0 20rpx 30rpx;}
.item_desc .icon{display:flex; align-items:center; width: 32rpx; height: 32rpx; margin-right: 10rpx; font-size: 0;}
.item_desc .icon .card_icon{width: 32rpx; height: 32rpx;}
.item_desc .name{flex: 1; font-size: 32rpx; line-height: 32rpx;}

/* 按钮 */
.wx_button_wrap{position:absolute;bottom:60rpx;left:0;right:0;padding:0 30rpx;}
.wx_button{
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  color:#ffffff;
  font-size:32rpx;
  border-radius:8rpx;
  height:100rpx;
  line-height:100rpx;
  box-shadow: 1rpx 12rpx 12rpx 0 rgba(19,52,255,0.07);
}
.wx_button.size_medium{width: 214rpx; height: 88rpx; line-height: 88rpx; font-size: 28rpx; margin:0;}
.wx_button.size_small{width: 145rpx; height: 50rpx; font-size: 24rpx; line-height: 50rpx; margin: 0;}
.wx_button_hover{opacity:0.95;}