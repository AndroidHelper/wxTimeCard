/* pages/order/orderDetail.wxss */
page{
    background: #F0F0F0;
}
.orderdetail_wrap{
    background: #FFFFFF;
}
.orderdetail_title{
    display: flex;
    padding: 34rpx 40rpx 16rpx 30rpx;
    border-bottom:2rpx solid #E4E4E4;
}
.orderdetail_title .left{
    flex: 1;
    text-align: left;
    font-size: 34rpx;
    line-height: 50rpx;
    color: #333333;
}
.orderdetail_title .right{
    flex: 1;
    text-align: right;
    font-size: 50rpx;
    line-height: 50rpx;
    color: #4F5EFF;
}
.orderdetail_wrap .orderdetail{
    font-size: 28rpx;
}
.orderdetail_wrap .orderdetail .name{
    color: #999999;
}
.orderdetail_wrap .orderdetail .desc{
    color: #333333;
}
.orderdetail_myCard{
    padding-top: 20rpx;
    text-align: center;
    font-size: 28rpx;
    color: #4F5EFF;
}